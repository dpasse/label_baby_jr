{
    "compilerOptions": {
      "jsx": "react",
      "target": "es5",
      "module": "es6",
      "outDir": "./dist/",
      "moduleResolution": "Node",
  
      "strict": false,
      "allowJs": true,
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "removeComments": true,
      "strictNullChecks": true,
      "noUncheckedIndexedAccess": true,
      "allowSyntheticDefaultImports": true,
  
      "rootDirs": [
        "app"
      ],
  
      "types": ["node"],
      "typeRoots": [
        "node_modules/@types",
      ],
    },
    "exclude": [
      "env",
      "web",
      "node_modules",
    ],
  }